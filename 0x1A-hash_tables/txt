
	curr = ht->array[index];
	if (curr == NULL)
		ht->array[index] = node;
	else
	{
		while (curr->next != NULL)
		{
			/* Same key? */
			if (strcmp(key, curr->key) == 0)
			{
				free(curr->value);
				curr->value = strdup(value);
				if (curr->value == NULL)
					return (0);
			}
			else
				curr = curr->next;
		}
		curr->next = node;
	}
