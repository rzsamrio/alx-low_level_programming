	while (curr->next != NULL)
		{
			/* Same key? */
			if (strcmp(key, curr->key) == 0)
			{
				free(curr->value);
				curr->value = strdup(value);
				if (curr->value == NULL)
					return (0);
			}
			else
				curr = curr->next;
		}
		/* Test last node */
		if (strcmp(key, curr->key) == 0)
		{
			free(curr->value);
			curr->value = strdup(value);
			if (curr->value == NULL)
				return (0);
		}
		else
			curr->next = node;
	}

